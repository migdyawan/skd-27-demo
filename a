def parse_nominal(text):
    text = text.lower().replace(".", "").replace(",", "")
    
    angka = re.search(r'(\d+)', text)
    if not angka:
        return 0
    
    nilai = int(angka.group(1))

    if "jt" in text:
        nilai *= 1000000
    elif "rb" in text:
        nilai *= 1000

    return nilai
